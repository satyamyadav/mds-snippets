{
	"name": "mds-snippets",
	"displayName": "MDS snippets",
	"description": "Masala Design System VSCode snippets",
	"version": "0.0.1",
	"publisher": "mds-dev",
	"repository": {},
	"engines": {
		"vscode": "^1.28.0"
	},
	"scripts": {
		"package": "vsce package",
		"install:plugin": "code --install-extension mds-snippets-0.0.1.vsix",
